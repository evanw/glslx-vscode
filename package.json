{
  "name": "glslx-vscode",
  "description": "A language service for GLSLX",
  "version": "0.1.8",
  "publisher": "evanw",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanw/glslx-vscode"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:glslx"
  ],
  "scripts": {
    "vscode:prepublish": "tsc"
  },
  "main": "./src/extension",
  "contributes": {
    "languages": [
      {
        "id": "glslx",
        "aliases": [
          "GLSLX",
          "glslx"
        ],
        "extensions": [
          ".glslx"
        ],
        "configuration": "./glslx.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "glslx",
        "scopeName": "source.glslx",
        "path": "./syntaxes/GLSLX.tmLanguage"
      }
    ]
  },
  "dependencies": {
    "glslx": "0.3.0",
    "vscode-languageclient": "7.0.0",
    "vscode-languageserver": "7.0.0",
    "vscode-languageserver-textdocument": "1.0.1",
    "vscode-uri": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "14.14.22",
    "@types/vscode": "1.52.0",
    "typescript": "4.1.3"
  }
}
